<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee"
         xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
         id="WebApp_ID" version="2.5">
  <display-name>GS data integration consumer</display-name>
  <servlet>
    <description>"GS data integration consumer"</description>
    <display-name>{{project.name}}PipelineConsumer</display-name>
    <servlet-name>pipelineConsumer</servlet-name>
    <servlet-class>com.gigaspaces.data_integration.{{project.name}}PipelineConsumerServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>pipelineConsumer</servlet-name>
    <url-pattern>pipeline-consumer</url-pattern>
    <!-- url will be http://<IP>:<PORT>/<PU_NAME>/pipeline-consumer-->
  </servlet-mapping>
</web-app>